<!-- Table saw table -->
    <div class="table">
        <table class="table table-striped table-hover tablesaw tablesaw-swipe tablesaw-sortable" data-tablesaw-mode="swipe" data-tablesaw-sortable="" data-tablesaw-sortable-switch="" data-tablesaw-minimap="" data-tablesaw-mode-switch="" style="">
                    <thead>
                        <tr>
                            <th scope="col" data-tablesaw-sortable-col="" data-tablesaw-priority="persist" class="tablesaw-cell-persist tablesaw-sortable-head"><button class="tablesaw-sortable-btn">email Address</button></th>
                            <th scope="col" data-tablesaw-sortable-col="" data-tablesaw-sortable-default-col="" data-tablesaw-priority="3" class="tablesaw-sortable-head tablesaw-sortable-ascending"><button class="tablesaw-sortable-btn">Joined as Subscriber at</button></th>
                       </tr>
                    </thead>
                    <tbody>
                        {{#each email}}
                        <tr>
                            <td>{{this.emailAddress}}</td>
                            <td>{{this.createdAt}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
    </div>